{
  "name": "@emmet/abbreviation",
  "version": "0.0.1",
  "description": "Emmet standalone abbreviation parser",
  "main": "dist/abbreviation.cjs.js",
  "jsnext:main": "dist/abbreviation.es.js",
  "scripts": {
    "test": "mocha",
    "build": "npm run build:cjs && npm run build:es",
    "build:cjs": "rollup -o dist/abbreviation.cjs.js -f cjs ./index.js",
    "build:es": "rollup -o dist/abbreviation.es.js -f es ./index.js",
    "prepublish": "npm run build"
  },
  "keywords": [
    "emmet",
    "abbreviation"
  ],
  "author": "Sergey Chikuyonok <serge.che@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-plugin-transform-es2015-modules-commonjs": "^6.18.0",
    "babel-register": "^6.18.0",
    "mocha": "^3.1.2",
    "rollup": "^0.36.3"
  },
  "directories": {
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emmetio/abbreviation.git"
  },
  "bugs": {
    "url": "https://github.com/emmetio/abbreviation/issues"
  },
  "homepage": "https://github.com/emmetio/abbreviation#readme"
}
